    "express-session": "^1.14.0",
    "lodash": "^4.15.0",
    "mongoose": "^4.5.8",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.3",
    "request": "^2.73.0",
    "rxjs": "5.0.0-beta.11",
    "session": "^0.1.0",
    "systemjs": "^0.19.38",
    "yelp": "^1.0.1",
    "zone.js": "^0.6.6"
